{
  "hosting": {
    "public": "frontend/web",
    "ignore": ["firebase.json", "**/.*"],
  "rewrites": [
    {
      "source": "/api/**",
      "run": {
        "serviceId": "fishermate-backend",
        "region": "us-central1"
      }
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
  }
}
