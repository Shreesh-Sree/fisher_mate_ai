# Analytics Configuration for FisherMate.AI
# Add these environment variables to your deployment

# Vercel Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_vercel_analytics_id_here

# Google Analytics (optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Custom Analytics API (optional)
NEXT_PUBLIC_ANALYTICS_API_URL=https://your-analytics-api.com
NEXT_PUBLIC_ANALYTICS_API_KEY=your_api_key_here

# Feature flags for analytics
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ANALYTICS_DEBUG=false

# Environment
NODE_ENV=production
